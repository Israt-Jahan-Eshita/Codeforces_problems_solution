#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int x,y,i,j,z,k,a,b,c;
    cin>>k;
    for(i=0;i<k;i++)
    {
        vector<long long int>v(3,0);
        for(j=0;j<3;j++)
        {
            cin>>v[j];
        }
        sort(v.begin(),v.end());
        if(v[1]!=v[2])
        {
            cout<<"NO"<<"\n";
        }
        else
        {
            cout<<"YES"<<"\n";
            cout<<v[0]<<" "<<v[0]<<" "<<v[2]<<"\n";
        }
    }
    return 0;
}
