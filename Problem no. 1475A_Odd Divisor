#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int x,y,i,j=0;
    cin>>x;
    for(i=0; i<x; i++)
    {
        cin>>y;
        j=0;
        if(y%2==0)
        {
            while(y>1)
            {
                y=y/2;
                if(y>1&&y%2!=0)
                {
                    j=1;
                    break;
                }
            }
            if(j==0)
            {
                cout<<"NO"<<"\n";
            }
            else
            {
                cout<<"YES"<<"\n";
            }
        }
        else
        {
            cout<<"YES"<<"\n";
        }
    }
    return 0;
}
