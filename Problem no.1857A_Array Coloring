#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int t,n,a[200],i,j,k;
    cin>>t;
    for(j=1;j<=t;j++)
    {
        cin>>n;
        k=0;
        for(i=0;i<n;i++)
        {
            cin>>a[i];
            k+=a[i];
        }
        if(k%2==0)
        {
            cout<<"YES"<<"\n";
        }
        else
        {
            cout<<"NO"<<"\n";
        }
    }
    return 0;
}
