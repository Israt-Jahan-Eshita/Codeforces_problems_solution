#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int x,i;
    string s;
    map<string,int>m;
    cin>>x;
    for(i=1;i<=x;i++)
    {
        cin>>s;
        m[s]++;
        if(m[s]==1)
        {
            cout<<"OK"<<"\n";
        }
        else
        {
            cout<<s<<m[s]-1<<"\n";
        }
    }
    return 0;
}
