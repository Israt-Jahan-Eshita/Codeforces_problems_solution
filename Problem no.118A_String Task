#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int i;
    string s;
    cin>>s;
    transform(s.begin(),s.end(),s.begin(),::tolower);
    for(i=0;i<s.length();i++)
    {
        if(s[i]!='a'&&s[i]!='e'&&s[i]!='i'&&s[i]!='o'&&s[i]!='u'&&s[i]!='y')
        {
            cout<<"."<<s[i];
        }
    }
    return 0;
}
